<div class="container">
  
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Interior Area</th>
      <th>Exterior Area:</th>
    </tr>
  </thead>
  <tbody>
    <% @sites.each do |site| %>
      <tr>
        <td><%= site.id %></td>
        <td><%= site.name %></td>
        <td><%= site.interior_area %></td>
        <td><%= site.exterior_area %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

 